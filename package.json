{
  "name": "@imagepix/client",
  "version": "1.0.0",
  "description": "Client library of imagepix that resize and convert images dynamically.",
  "repository": {
    "type": "git",
    "url": "https://github.com/imagepix/client-js.git"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "http-server -o coverage/lcov-report",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "typedoc": "typedoc --options typedoc.json",
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:common": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --max-warnings 0",
      "prettier --write --loglevel warn",
      "jest --findRelatedTests"
    ]
  },
  "keywords": [
    "image",
    "resize",
    "crop",
    "convert",
    "dynamic"
  ],
  "author": "Smart Algorithm Co., Ltd. <contact@smartalgorithm.co.jp>",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
